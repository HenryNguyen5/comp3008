{
	"rules": {
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"no-unused-variable": true,
		"curly": true,
		"class-name": true,
		"prefer-const": true,
		"semicolon": ["always"],
		"triple-equals": true,
		"no-var-keyword": true,
		"no-require-imports": false,
		"variable-name": [
			true,
			"ban-keywords"
		]
		
	}
}